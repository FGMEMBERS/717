	<!-- Gear indicator lights -->
	<animation>
		<name>Left main gear locked light</name>
		<type>select</type>
		<object-name>GearLLocked</object-name>
		<condition>
			<or>
				<equals>
					<property>gear/gear[1]/position-norm</property>
					<value>0</value>
				</equals>
				<equals>
					<property>gear/gear[1]/position-norm</property>
					<value>1</value>
				</equals>
			</or>
		</condition>
	</animation>
	<animation>
		<name>Left main gear in-transit light</name>
		<type>select</type>
		<object-name>GearLTransit</object-name>
		<condition>
			<and>
				<greater-than>
					<property>gear/gear[1]/position-norm</property>
					<value>0</value>
				</greater-than>
				<less-than>
					<property>gear/gear[1]/position-norm</property>
					<value>1</value>
				</less-than>
			</and>
		</condition>
	</animation>

	<animation>
		<name>Nose gear locked light</name>
		<type>select</type>
		<object-name>GearNLocked</object-name>
		<condition>
			<or>
				<equals>
					<property>gear/gear[0]/position-norm</property>
					<value>0</value>
				</equals>
				<equals>
					<property>gear/gear[0]/position-norm</property>
					<value>1</value>
				</equals>
			</or>
		</condition>
	</animation>
	<animation>
		<name>Nose gear in-transit light</name>
		<type>select</type>
		<object-name>GearNTransit</object-name>
		<condition>
			<and>
				<greater-than>
					<property>gear/gear[0]/position-norm</property>
					<value>0</value>
				</greater-than>
				<less-than>
					<property>gear/gear[0]/position-norm</property>
					<value>1</value>
				</less-than>
			</and>
		</condition>
	</animation>

	<animation>
		<name>Right main gear locked light</name>
		<type>select</type>
		<object-name>GearRLocked</object-name>
		<condition>
			<or>
				<equals>
					<property>gear/gear[2]/position-norm</property>
					<value>0</value>
				</equals>
				<equals>
					<property>gear/gear[2]/position-norm</property>
					<value>1</value>
				</equals>
			</or>
		</condition>
	</animation>
	<animation>
		<name>Right main gear in-transit light</name>
		<type>select</type>
		<object-name>GearRTransit</object-name>
		<condition>
			<and>
				<greater-than>
					<property>gear/gear[2]/position-norm</property>
					<value>0</value>
				</greater-than>
				<less-than>
					<property>gear/gear[2]/position-norm</property>
					<value>1</value>
				</less-than>
			</and>
		</condition>
	</animation>

	<!-- Yokes -->
	<animation>
		<name>Yoke elevator</name>
		<type>rotate</type>
		<object-name>YokeColL</object-name>
		<object-name>YokeL</object-name>
		<object-name>YokeColR</object-name>
		<object-name>YokeR</object-name>
		<property>controls/flight/elevator</property>
		<factor>10</factor>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>-1.474</x-m>
			<y-m>0</y-m>
			<z-m>0.19</z-m>
		</center>
	</animation>
	<animation>
		<name>Left yoke aileron</name>
		<type>rotate</type>
		<object-name>YokeL</object-name>
		<property>controls/flight/aileron</property>
		<factor>90</factor>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>0</x-m>
			<y-m>-0.5</y-m>
