		<object-name>SpdBrkLvr</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>controls/flight/speedbrake-lever</property>
				<step type="double">1</step>
				<max>2</max>
			</binding>
		</action>
		<action>
			<button>1</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>controls/flight/speedbrake-lever</property>
				<step type="double">-1</step>
				<min>0</min>
			</binding>
		</action>
	</animation>
