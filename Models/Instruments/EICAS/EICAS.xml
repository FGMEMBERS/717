	<!-- Emission animations -->
	<animation>
		<name>White text</name>
		<type>material</type>
		<object-name>TAT</object-name>
		<object-name>EPR</object-name>
		<object-name>EPR L</object-name>
		<object-name>EPR R</object-name>
		<object-name>N1</object-name>
		<object-name>N1 L</object-name>
		<object-name>N1 R</object-name>
		<object-name>TGT</object-name>
		<object-name>N2</object-name>
		<object-name>N2 L</object-name>
		<object-name>N2 R</object-name>
		<object-name>FF</object-name>
		<object-name>FF Units</object-name>
		<object-name>FF L</object-name>
		<object-name>FF R</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
		</emission>
	</animation>
	<animation>
		<name>Blue text</name>
		<type>material</type>
		<object-name>Total fuel</object-name>
		<emission>
			<red>0.32</red>
			<green>0.66</green>
			<blue>0.91</blue>
		</emission>
	</animation>

	<!-- ANIMATIONS -->
	<!-- Lighting -->
	<animation>
		<name>Cockpit illumination</name>
		<type>material</type>
		<object-name>Border</object-name>
		<object-name>Off</object-name>
		<emission>
			<red>0.5</red>
			<green>0.5</green>
			<blue>0.5</blue>
			<factor-prop>controls/lighting/dome-norm</factor-prop>
		</emission>
	</animation>

	<!-- Power on/off -->
	<animation>
		<name>Power on/off</name>
		<type>select</type>
		<object-name>Off</object-name>
		<condition>
			<less-than>
				<property>systems/electrical/outputs/efis</property>
				<value>15</value>
			</less-than>
		</condition>
	</animation>

	<!-- EPR -->
	<animation>
		<name>Left EPR</name>
		<type>rotate</type>
		<object-name>EPRLPtr</object-name>
		<property>engines/engine[0]/epr</property>
		<interpolation>
			<entry><ind>1</ind><dep>63</dep></entry>
			<entry><ind>3</ind><dep>-140</dep></entry>
		</interpolation>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>0</x-m>
			<y-m>-0.04</y-m>
			<z-m>0.074</z-m>
		</center>
	</animation>
