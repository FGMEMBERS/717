
<PropertyList>

 <path>SD.ac</path>
 
<animation>
    <type>scale</type>
    <x-offset>1.2</x-offset>
    <y-offset>1.2</y-offset>
    <z-offset>1.2</z-offset>
</animation>

    <animation>
        <type>select</type>
        <object-name>Off</object-name>
        <condition>
            <less-than>
				<property>systems/electrical/outputs/efis</property>
				<value>15</value>
            </less-than>
        </condition>
    </animation>
 
</PropertyList>
