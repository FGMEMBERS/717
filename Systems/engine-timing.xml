	<filter>
		<name>Throttle L</name>
		<debug>false</debug>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<input>
			<property>/controls/engines/engine[0]/throttle-fdm</property>
		</input>
		<output>/controls/engines/engine[0]/throttle-lever</output>
		<type>noise-spike</type>
		<max-rate-of-change>
			<condition>
				<equals>
					<property>/it-autoflight/output/athr</property>
					<value>0</value>
				</equals>
			</condition>
			<value>0.95</value>
		</max-rate-of-change>
		<max-rate-of-change>
			<condition>
				<equals>
					<property>/it-autoflight/output/athr</property>
					<value>1</value>
				</equals>
			</condition>
			<value>2</value>
		</max-rate-of-change>
	</filter>
	
	<filter>
		<name>Throttle R</name>
		<debug>false</debug>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<input>
			<property>/controls/engines/engine[1]/throttle-fdm</property>
		</input>
		<output>/controls/engines/engine[1]/throttle-lever</output>
		<type>noise-spike</type>
		<max-rate-of-change>
			<condition>
				<equals>
					<property>/it-autoflight/output/athr</property>
					<value>0</value>
				</equals>
			</condition>
			<value>0.95</value>
		</max-rate-of-change>
		<max-rate-of-change>
			<condition>
				<equals>
					<property>/it-autoflight/output/athr</property>
					<value>1</value>
				</equals>
			</condition>
			<value>2</value>
		</max-rate-of-change>
	</filter>

</PropertyList>
