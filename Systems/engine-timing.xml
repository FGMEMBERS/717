        <gain>1</gain>
        <update-interval-secs type="double">0.05</update-interval-secs>
        <input> 
            <expression>
                <table>
                    <property>/engines/engine[0]/rpm</property> 
                    <entry><ind>0</ind><dep>0</dep></entry>
                    <entry><ind>20</ind><dep>42</dep></entry>
                    <entry><ind>105</ind><dep>53</dep></entry>
