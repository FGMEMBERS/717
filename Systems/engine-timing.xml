    <filter>
        <name>Oil Temp R</name>
        <type>gain</type>
        <gain>1</gain>
        <update-interval-secs type="double">0.05</update-interval-secs>
        <input> 
            <expression>
                <table>
                    <property>/engines/engine[1]/egt-actual</property> 
                    <entry><ind>0</ind><dep>0</dep></entry>
                    <entry><ind>302</ind><dep>63</dep></entry>
