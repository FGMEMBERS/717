<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

    <filter>
        <name>EPR L</name>
        <type>gain</type>
        <gain>1</gain>
        <update-interval-secs type="double">0.05</update-interval-secs>
        <input> 
            <expression>
                <table>
                    <property>/engines/engine[0]/rpm</property> 
                    <entry><ind>   18</ind><dep>0.988</dep></entry>
                    <entry><ind>   25</ind><dep>1.000</dep></entry>
                    <entry><ind>   35</ind><dep>1.060</dep></entry>
                    <entry><ind>105.8</ind><dep>1.542</dep></entry>
                </table>
            </expression>
        </input>
		<output>/engines/engine[0]/epr-actual</output>
    </filter>
	
    <filter>
        <name>EPR R</name>
        <type>gain</type>
        <gain>1</gain>
        <update-interval-secs type="double">0.05</update-interval-secs>
        <input> 
            <expression>
                <table>
                    <property>/engines/engine[1]/rpm</property> 
                    <entry><ind>   18</ind><dep>0.988</dep></entry>
                    <entry><ind>   25</ind><dep>1.000</dep></entry>
                    <entry><ind>   35</ind><dep>1.060</dep></entry>
                    <entry><ind>105.8</ind><dep>1.542</dep></entry>
                </table>
            </expression>
        </input>
		<output>/engines/engine[1]/epr-actual</output>
    </filter>

    <filter>
        <name>EGT L</name>
